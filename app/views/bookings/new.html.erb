<div class="container mt-4">
<h1>Book</h1>
<%= simple_form_for [@service, @booking] do |f| %>
  <%= f.association :service %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.button :submit %>
<% end %>
<%= link_to "<< Back to all services", services_path, class: "btn btn-flat mt-3" %>
</div>
